[env:uno]  # Replace "uno" with your board (e.g., "nano", "esp32dev", etc.)
platform = espressif8266  # Use "espressif32" for ESP32 or "espressif8266" for ESP8266
board = esp12e
framework = arduino
monitor_speed = 115200  # Baud rate for serial monitor
lib_deps =
    adafruit/Adafruit INA219@^1.1.1
    adafruit/Adafruit BusIO@^1.14.1
